/* pages/release/release.wxss */

.cu-form-group .title {
  min-width: calc(4em + 30rpx);
}
.textBack{
  display: flex;
  margin: 20rpx;
  margin-left: 30rpx;
  margin-right: 30rpx;
  height: 32vh;
  width: calc(100vw - 60rpx);
}
.imageSelect{
  display: flex;
  margin: 20rpx 0 0 30rpx;
  width: 180rpx;
  height: 180rpx;
  background-color: #9991;
}

.release{
  display: flex;
  margin: 80rpx 26vw 0 26vw;
  margin-bottom: 20rpx;
  width: 48vw;
  height: 80rpx;
  align-items: center;
  justify-content: center;
  background: linear-gradient(to right,#ad6cef, #846ff0);
}